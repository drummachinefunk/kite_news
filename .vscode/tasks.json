{
  "version": "2.0.0",
  "tasks": [
    {
      "label":" Build Flutter iOS",
      "type": "shell",
      "command": "flutter",
      "args": [
        "build",
        "ipa",
      ],
      "group": "build",
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared"
      },
      "problemMatcher": []
    },
    {
      "label":" Build Flutter Android",
      "type": "shell",
      "command": "flutter",
      "args": [
        "build",
        "appbundle",
      ],
      "group": "build",
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared"
      },
      "problemMatcher": []
    },
    {
      "label":"Generate localization",
      "type": "shell",
      "command": "flutter",
      "args": [
        "gen-l10n",
      ],
      "group": "build",
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared"
      },
      "problemMatcher": []
    },
    {
      "label": "Dump Flutter Licenses",
      "type": "shell",
      "command": "flutter",
      "args": [
        "run",
        "-t",
        "tools/dump_licenses.dart",
        "--device-id",
        "flutter-tester"
      ],
      "group": "build",
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared"
      },
      "problemMatcher": []
    }
  ]
}
